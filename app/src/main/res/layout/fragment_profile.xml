<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:scrollbars="none"
    tools:context=".fragment.ProfileFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:paddingBottom="@dimen/_80sdp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_8sdp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/_128sdp"
                android:background="@color/green"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tvUserName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Jhon Deo"
                    android:fontFamily="@font/mulish_extrabold"
                    android:textSize="@dimen/_18sdp"
                    android:textColor="@color/black_b"
                    android:textFontWeight="800"
                    android:layout_marginLeft="@dimen/_15sdp"
                    android:layout_marginTop="@dimen/_33sdp"/>

                <TextView
                    android:id="@+id/tvPhoneEmail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="+61 42226335897   Jhon212@gmail.com"
                    android:fontFamily="@font/mulish_light"
                    android:textSize="@dimen/_12sdp"
                    android:layout_marginTop="@dimen/_9sdp"
                    android:layout_marginLeft="@dimen/_15sdp"
                    android:textColor="@color/black_b"
                    android:textFontWeight="300"/>

            </LinearLayout>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="@dimen/_55sdp"
                android:layout_marginTop="@dimen/_90sdp"
                app:cardCornerRadius="@dimen/_5sdp"
                app:cardElevation="@dimen/_3sdp"
                android:layout_marginBottom="@dimen/_12sdp"
                android:layout_marginHorizontal="@dimen/_15sdp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/_18sdp"
                        android:textColor="@color/black_b"
                        android:layout_marginLeft="@dimen/_12sdp"
                        android:layout_gravity="center_vertical"
                        android:fontFamily="@font/mulish_medium"
                        android:textFontWeight="500"
                        android:text="My Account"/>
                </LinearLayout>



            </androidx.cardview.widget.CardView>




        </RelativeLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/llProfile"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/_16sdp">
                <ImageView
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_16sdp"
                    android:src="@drawable/ic_vector__52_"
                    android:layout_marginLeft="@dimen/_16sdp"
                    android:layout_gravity="center_vertical"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Profile"
                    android:textSize="@dimen/_15sdp"
                    android:textFontWeight="500"
                    android:layout_marginStart="@dimen/_15sdp"
                    android:fontFamily="@font/mulish_medium"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_marginHorizontal="@dimen/_16sdp"
                android:background="@drawable/doted_line"
                android:layout_marginTop="@dimen/_12sdp"/>

            <LinearLayout
                android:id="@+id/llSubscribenewStore"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/_20sdp">
                <ImageView
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_16sdp"
                    android:src="@drawable/ic_vector__54_"
                    android:layout_marginLeft="@dimen/_16sdp"
                    android:layout_gravity="center_vertical"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Subscribe New Store"
                    android:textSize="@dimen/_15sdp"
                    android:textFontWeight="500"
                    android:layout_marginStart="@dimen/_15sdp"
                    android:fontFamily="@font/mulish_medium"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_marginHorizontal="@dimen/_16sdp"
                android:background="@drawable/doted_line"
                android:layout_marginTop="@dimen/_12sdp"/>

            <LinearLayout
                android:id="@+id/llSTD"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/_20sdp">
                <ImageView
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_16sdp"
                    android:src="@drawable/ic_group_871"
                    android:layout_marginLeft="@dimen/_16sdp"
                    android:layout_gravity="center_vertical"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Switch to Dealer"
                    android:textSize="@dimen/_15sdp"
                    android:textFontWeight="500"
                    android:layout_marginStart="@dimen/_15sdp"
                    android:fontFamily="@font/mulish_medium"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_marginHorizontal="@dimen/_16sdp"
                android:background="@drawable/doted_line"
                android:layout_marginTop="@dimen/_12sdp"/>

            <LinearLayout
                android:id="@+id/llTAC"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/_20sdp">
                <ImageView
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_16sdp"
                    android:src="@drawable/ic_vector__55_"
                    android:layout_marginLeft="@dimen/_16sdp"
                    android:layout_gravity="center_vertical"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Terms &amp; Conditions"
                    android:textSize="@dimen/_15sdp"
                    android:textFontWeight="500"
                    android:layout_marginStart="@dimen/_15sdp"
                    android:fontFamily="@font/mulish_medium"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_marginHorizontal="@dimen/_16sdp"
                android:background="@drawable/doted_line"
                android:layout_marginTop="@dimen/_12sdp"/>

            <LinearLayout
                android:id="@+id/llPP"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/_20sdp">
                <ImageView
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_16sdp"
                    android:src="@drawable/ic_group_872"
                    android:layout_marginLeft="@dimen/_16sdp"
                    android:layout_gravity="center_vertical"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Privacy Policy"
                    android:textSize="@dimen/_15sdp"
                    android:textFontWeight="500"
                    android:layout_marginStart="@dimen/_15sdp"
                    android:fontFamily="@font/mulish_medium"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_marginHorizontal="@dimen/_16sdp"
                android:background="@drawable/doted_line"
                android:layout_marginTop="@dimen/_12sdp"/>

            <LinearLayout
                android:id="@+id/llAU"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/_20sdp">
                <ImageView
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_16sdp"
                    android:src="@drawable/ic_group_882"
                    android:layout_marginLeft="@dimen/_16sdp"
                    android:layout_gravity="center_vertical"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="About Us"
                    android:textSize="@dimen/_15sdp"
                    android:textFontWeight="500"
                    android:layout_marginStart="@dimen/_15sdp"
                    android:fontFamily="@font/mulish_medium"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_marginHorizontal="@dimen/_16sdp"
                android:background="@drawable/doted_line"
                android:layout_marginTop="@dimen/_12sdp"/>
            <LinearLayout
                android:id="@+id/llLogout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/_20sdp">
                <ImageView
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_16sdp"
                    android:src="@drawable/ic_vector__57_"
                    android:layout_marginLeft="@dimen/_16sdp"
                    android:layout_gravity="center_vertical"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Logout"
                    android:textSize="@dimen/_15sdp"
                    android:textFontWeight="500"
                    android:layout_marginStart="@dimen/_15sdp"
                    android:fontFamily="@font/mulish_medium"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_marginHorizontal="@dimen/_16sdp"
                android:background="@drawable/doted_line"
                android:layout_marginTop="@dimen/_12sdp"/>



        </LinearLayout>

    </LinearLayout>


</ScrollView>